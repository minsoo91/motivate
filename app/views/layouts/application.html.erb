<!DOCTYPE html>
<html>
<head>
  <title>Motivate</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if flash[:errors] %>
	<%= flash.now[:errors] %>
<% end %>
<% if current_user %>
	<%= link_to "Sign Out", session_url, method: :delete  %>
<% end %>
<%= yield %>

</body>
</html>
